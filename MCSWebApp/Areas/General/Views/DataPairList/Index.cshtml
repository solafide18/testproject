<div class="row">
    <div class="col-xl-12">
        <div id="panel-1" class="panel">
            <div class="panel-hdr">
                <h2>
                    @ViewBag.Breadcrumb
                </h2>
                <div class="panel-toolbar">
                    <button class="btn btn-panel" data-action="panel-collapse" data-toggle="tooltip" data-offset="0,10" data-original-title="Collapse"></button>
                    <button class="btn btn-panel" data-action="panel-fullscreen" data-toggle="tooltip" data-offset="0,10" data-original-title="Fullscreen"></button>
                </div>
            </div>
            <div class="panel-container show">
                <div class="panel-content">
                    <div class="row">
                        <div class="col-xl-12">
                            <ul class="nav nav-tabs nav-tabs-clean" role="tablist">
                                <li class="nav-item"><a class="nav-link active" data-toggle="tab" href="#tab-locations" role="tab">Locations</a></li>
                                @*<li class="nav-item"><a class="nav-link" data-toggle="tab" href="#tab-bank-and-currency" role="tab">Bank & Currency</a></li>*@
                                <li class="nav-item"><a class="nav-link" data-toggle="tab" href="#customer" role="tab">Customer</a></li>
                            </ul>
                            <div class="tab-content p-3">
                                <!-- Locations -->
                                <div class="tab-pane fade show active" id="tab-locations" role="tabpanel" aria-labelledby="tab-home">
                                    <!-- Country -->
                                    <div class="row mb-3">
                                        <div class="col-md-2">
                                            <div class="list-head">
                                                <div class="row">
                                                    <div class="col-md-4">
                                                        <div class="head-icon">
                                                            <i class="fas fa-flag"></i>
                                                        </div>
                                                    </div>
                                                    <div class="col-md-8">
                                                        <div class="head-text">
                                                            <small class="text-uppercase">Location</small>
                                                            <h3 class="font-weight-bold">Country</h3>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-md-10">
                                            <div id="country-grid"></div>
                                        </div>
                                    </div>

                                    <!-- Province -->
                                    <div class="row mb-3">
                                        <div class="col-md-2">
                                            <div class="list-head">
                                                <div class="row">
                                                    <div class="col-md-4">
                                                        <div class="head-icon">
                                                            <i class="fas fa-map"></i>
                                                        </div>
                                                    </div>
                                                    <div class="col-md-8">
                                                        <div class="head-text">
                                                            <small class="text-uppercase">Location</small>
                                                            <h3 class="font-weight-bold">Province</h3>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-md-10">
                                            <div id="province-grid"></div>
                                        </div>
                                    </div>

                                    <!-- City -->
                                    <div class="row mb-3">
                                        <div class="col-md-2">
                                            <div class="list-head">
                                                <div class="row">
                                                    <div class="col-md-4">
                                                        <div class="head-icon">
                                                            <i class="fas fa-map-pin"></i>
                                                        </div>
                                                    </div>
                                                    <div class="col-md-8">
                                                        <div class="head-text">
                                                            <small class="text-uppercase">Location</small>
                                                            <h3 class="font-weight-bold">City</h3>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-md-10">
                                            <div class="dx-viewport">
                                                <div class="demo-container">
                                                    <div id="city-grid"></div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>

                                <!-- Bank and Currency -->
                                <div class="tab-pane fade" id="tab-bank-and-currency" role="tabpanel" aria-labelledby="tab-profile">
                                    <div class="row">
                                        <div class="col-md-6">
                                            <div class="dx-viewport">
                                                <div class="demo-container">
                                                    <div id="grid"></div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>

                                <!-- Customer -->
                                <div class="tab-pane fade" id="customer" role="tabpanel" aria-labelledby="tab-profile">
                                    <div class="row mb-3">
                                        <div class="col-md-2">
                                            <div class="list-head">
                                                <div class="row">
                                                    <div class="col-md-4">
                                                        <div class="head-icon">
                                                            <i class="fas fa-user"></i>
                                                        </div>
                                                    </div>
                                                    <div class="col-md-8">
                                                        <div class="head-text">
                                                            <small class="text-uppercase">Customer</small>
                                                            <h3 class="font-weight-bold">Customer Type</h3>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-md-10">
                                            <div id="customer-type-grid"></div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

@section ScriptsBlock
{
    <script src="https://cdnjs.cloudflare.com/ajax/libs/babel-polyfill/7.4.0/polyfill.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jszip/3.1.2/jszip.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/exceljs/3.3.1/exceljs.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/FileSaver.js/1.3.8/FileSaver.min.js"></script>
    <script src="~/js/devextreme/dx.all.js"></script>
    <script src="~/js/devextreme/aspnet/dx.aspnet.data.js"></script>
    <script defer src="~/app/areas/general/data_pair_list/js/index.js?_=@DateTime.Now.Ticks"></script>
}